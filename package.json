{
  "name": "skillstak",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "dev:cf": "CLOUDFLARE_MODE=true react-router dev",
    "dev:wrangler": "npm run build && wrangler pages dev ./build/client --compatibility-date=2024-11-11 --live-reload",
    "build": "CLOUDFLARE_MODE=true react-router build",
    "build:node": "react-router build",
    "start": "react-router start",
    "cf:pages": "wrangler pages dev --compatibility-date=2024-11-11 ./build/client",
    "d1:apply-prod": "wrangler d1 execute SKILLSTAK_DB --file=./db/schema.sql --remote",
    "d1:create-prod": "wrangler d1 create skillstak-db",
    "d1:export-prod": "node scripts/export-prod-db.js",
    "d1:apply:local": "wrangler d1 execute SKILLSTAK_DB --local --file=./db/schema.sql",
    "d1:init:node": "node scripts/init-local-db.js",
    "d1:export-local": "node scripts/export-local-db.js",
    "d1:delete-local": "rm -rf .wrangler/state/",
    "d1:reset-local": "npm run d1:delete-local && npm run d1:apply:local",
    "d1:reset-node": "npm run d1:delete-local && npm run d1:init:node",
    "migrate:local": "node scripts/migrate-local.js",
    "migrate:prod": "node scripts/migrate-prod.js",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "ci": "biome ci .",
    "kill-port": "node ./scripts/kill-port.cjs 8788",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:node": "TEST_ENV=node playwright test",
    "test:e2e:cf": "TEST_ENV=cloudflare playwright test"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/cloudflare": "^7.0.0",
    "@react-router/dev": "^7.0.0",
    "@react-router/serve": "^7.0.0",
    "@sentry/cloudflare": "^10.22.0",
    "@sentry/node": "^10.22.0",
    "@sentry/react": "^10.22.0",
    "@sentry/tracing": "^7.120.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "isbot": "^5.1.31",
    "lucide-react": "^0.546.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^16.2.0",
    "react-router": "^7.9.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "ulidx": "^2.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@cloudflare/workers-types": "^4.20251011.0",
    "@playwright/test": "^1.56.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^12.4.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "wrangler": "^4.43.0"
  }
}
